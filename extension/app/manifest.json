{
    "name": "Compass",
    "version": "0.5.0",
    "description": "Password manager for Chrome",
    "icons": {
        "128": "compass_locked.png"
    },
    "browser_action": {
        "default_title": "Compass",
        "default_icon": "compass_action.png",
        "default_popup": "index.html"
    },
    "background": {
        "page": "background.html"
    },
    "permissions": [
        "clipboardWrite",
        "activeTab"
    ],
    "manifest_version": 2
}
