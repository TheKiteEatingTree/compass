{
    "name": "Compass",
    "version": "1.0",
    "description": "Password manager for Chrome",
    "icons": {
        "128": "news_icon.png"
    },
    "browser_action": {
        "default_title": "Compass",
        "default_icon": "news_action.png",
        "default_popup": "index.html"
    },
    "background": {
        "page": "background.html"
    },
    "permissions": [
        "clipboardWrite",
        "activeTab"
    ],
    "manifest_version": 2
}
