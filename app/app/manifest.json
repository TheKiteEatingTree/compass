{
    "name": "Compass Base",
    "version": "1.0",
    "description": "Password manager for Chrome",
    "icons": {
        "128": "news_icon.png"
    },
    "app": {
        "background": {
            "scripts": ["dist/background.bundle.js"]
        }
    },
    "permissions": [
        "storage",
        "clipboardWrite",
        {"fileSystem": ["write", "retainEntries", "directory"]}
    ],
    "manifest_version": 2
}
