{
    "name": "compass",
    "version": "1.0",
    "description": "Displays the first 5 items from the 'Google News - top news' RSS feed in a popup.",
    "icons": {
        "128": "news_icon.png"
    },
    "app": {
        "background": {
            "scripts": ["background.js"]
        }
    },
    "permissions": [
        "storage",
        "clipboardWrite",
        {"fileSystem": ["write", "retainEntries", "directory"]}
    ],
    "manifest_version": 2
}
